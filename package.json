{
  "name": "gundam-names",
  "version": "1.0.0",
  "description": "Repository and generator of Gundam character names.",
  "main": "index.js",
  "scripts": {
    "build": "npm run generate-names-json && rollup -c",
    "test": "jest --verbose=false --runInBand",
    "generate-names-json": "node scripts/generate-names-json.js",
    "docs:build": "rollup --config rollup.config.docs.js",
    "docs:dev": "rollup --config rollup.config.docs.js -w",
    "docs:start": "sirv docs --no-clear"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fauntleroy/gundam-names.git"
  },
  "keywords": [
    "gundam",
    "names",
    "amuro"
  ],
  "author": "Timothy Kempf <tim@kempf.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Fauntleroy/gundam-names/issues"
  },
  "homepage": "https://fauntleroy.github.io/gundam-names/",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/pluginutils": "^3.1.0",
    "babel-jest": "^27.0.6",
    "csv-parse": "^4.16.0",
    "jest": "^27.0.6",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
}
